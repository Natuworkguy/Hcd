<feature
      id="org.sonatype.m2e.egit.feature"
      label="Maven SCM Handler for EGit"
      version="0.16.0.qualifier"
      provider-name="Takari, Inc."
      plugin="org.sonatype.m2e.egit">

   <description url="https://m2eclipse.sonatype.org/">
      Checkout Maven projects from Git using EGit.
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="Maven Integration Extras" url="https://m2eclipse.sonatype.org/sites/m2e-extras/"/>
   </url>

   <requires>
      <import feature="org.eclipse.platform"/>
      <import feature="org.eclipse.m2e.feature"/>
      <import feature="org.eclipse.egit"/>
      <import feature="org.eclipse.jgit"/>
   </requires>

   <plugin
         id="org.sonatype.m2e.egit"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
